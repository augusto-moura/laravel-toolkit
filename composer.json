{
    "name": "augusto-moura/laravel-toolkit",
    "description": "Adds macros to various Laravel classes and provides helper classes, as well as supercharged utility classes.",
    "type": "library",
	"license": "MIT",
	"require": {
		"php": ">= 8.2",
		"laravel/framework": "^10.0|^11.0",
        "wa72/htmlpagedom": "2.0.0",
        "guzzlehttp/guzzle": "^7.8"
	},
    "require-dev": {
        "orchestra/testbench": "^8.15"
    },
	"autoload": {
		"psr-4": {
			"AugustoMoura\\LaravelToolkit\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"AugustoMoura\\LaravelToolkit\\Providers\\LaravelToolkitServiceProvider"
			]
		}
	},
    "authors": [
        {
            "name": "Augusto Moura",
            "email": "augusto_moura40@hotmail.com"
        }
    ]
}
